{
  "name": "case-converter",
  "displayName": "Case Converter",
  "publisher": "Fuzionix",
  "description": "Quick case conversion with multiple case styles support and keyboard shortcuts",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuzionix/vscode-case-converter.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "vscode-extension",
    "converter",
    "text",
    "case",
    "case-change"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "case-converter.toPrevCase",
        "title": "Convert to Previous Case"
      },
      {
        "command": "case-converter.toNextCase",
        "title": "Convert to Next Case"
      },
      {
        "command": "case-converter.toOriginalCase",
        "title": "Original Case"
      },
      {
        "command": "case-converter.toConstCase",
        "title": "CONSTANT_CASE"
      },
      {
        "command": "case-converter.toPascalCase",
        "title": "PascalCase"
      },
      {
        "command": "case-converter.toCamelCase",
        "title": "camelCase"
      },
      {
        "command": "case-converter.toSnakeCase",
        "title": "snake_case"
      },
      {
        "command": "case-converter.toKebabCase",
        "title": "kebab-case"
      }
    ],
    "keybindings": [
      {
        "command": "case-converter.toPrevCase",
        "key": "alt+a",
        "when": "editorTextFocus"
      },
      {
        "command": "case-converter.toNextCase",
        "key": "alt+d",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Case Converter",
      "properties": {
        "caseConverter.showPopup": {
          "type": "boolean",
          "default": false,
          "description": "Show notification popup when case conversion is performed"
        },
        "caseConverter.caseCycle": {
          "type": "array",
          "default": [
            "original",
            "const",
            "camel",
            "snake",
            "kebab"
          ],
          "description": "Configure the order of cases to cycle through",
          "items": {
            "type": "string",
            "enum": [
              "original",
              "const",
              "camel",
              "pascal",
              "snake",
              "kebab"
            ]
          }
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "submenu": "case-converter.caseMenu",
          "group": "1_modification@500"
        }
      ],
      "case-converter.caseMenu": [
        {
          "command": "case-converter.toOriginalCase",
          "group": "1_cases@1"
        },
        {
          "command": "case-converter.toConstCase",
          "group": "1_cases@2"
        },
        {
          "command": "case-converter.toPascalCase",
          "group": "1_cases@3"
        },
        {
          "command": "case-converter.toCamelCase",
          "group": "1_cases@4"
        },
        {
          "command": "case-converter.toSnakeCase",
          "group": "1_cases@5"
        },
        {
          "command": "case-converter.toKebabCase",
          "group": "1_cases@6"
        }
      ]
    },
    "submenus": [
      {
        "id": "case-converter.caseMenu",
        "label": "Convert Case"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.4.5"
  }
}